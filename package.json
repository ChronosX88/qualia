{
  "name": "qualia",
  "productName": "qualia",
  "version": "1.0.0",
  "description": "aa",
  "main": "./.webpack/main",
  "scripts": {
    "build:webapp": "yarn run webpack --config webpack.web.config.mjs",
    "start:webapp": "yarn webpack serve --config webpack.web.config.mjs",
    "build:server": "yarn webpack --env buildMode=server --mode='development' --config webpack.web.config.js; ln -sTf $(realpath release/renderer) src/server/build; cd src/server; yarn",
    "start:server": "yarn node ./src/server/index.js",
    "start:electron": "yarn run electron-forge start",
    "build:electron": "yarn run electron-forge make",
    "lint": "eslint .",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
  "author": {
    "name": "fetsorn",
    "email": "fetsorn@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": ["darwin", "win32", "linux"]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        {
          "name": "@electron-forge/plugin-webpack",
          "devContentSecurityPolicy": "Content-Security-Policy: script-src 'self'",
          "config": {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "name": "main_window",
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/renderer.jsx",
                  "preload": {
                    "js": "./src/renderer/preload.js"
                  }
                }
              ]
            }
          }
        },
        {
          "name": "@timfish/forge-externals-plugin",
          "config": {
            "externals": ["electron", "electron-devtools-installer"],
            "includeDeps": true
          }
        }
      ]
    }
  },
  "devDependencies": {
